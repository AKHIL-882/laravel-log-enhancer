{
  "name": "akhilduggirala/laravel-log-enhancer",
  "description": "Enhanced logging helpers for Laravel (smart message/context normalization).",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Akhil",
      "email": "akhilduggirala882@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "AkhilDuggirala\\LaravelLogEnhancer\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "AkhilDuggirala\\LaravelLogEnhancer\\LogEnhancerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^9.15",
    "phpunit/phpunit": "^11.5"
  }
}

